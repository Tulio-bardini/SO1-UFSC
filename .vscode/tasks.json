{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: g++-11 build active file",
            "command": "/home/linuxbrew/.linuxbrew/bin/g++-11",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${fileDirname}/cpu.cc",
                "${fileDirname}/debug.cc",
                "${fileDirname}/system.cc",
                "${fileDirname}/thread.cc",
                "${fileDirname}/main.cc",
                "${fileDirname}/main_class.cc",
                "-o",
                "${fileDirname}/main"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}